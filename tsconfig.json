{
  "compilerOptions": {
    "jsx": "react-jsx", // أو "react" إذا كنت تستخدم React أقدم من الإصدار 17
    "target": "es6", // دعم ميزات JavaScript الحديثة
    "module": "esnext", // استخدام نمط الوحدات الحديثة
    "strict": true, // للتحقق الصارم من الأنواع
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
